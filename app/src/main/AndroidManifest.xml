<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.edu.bsi.projetoquizalgoritmo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo_oficial_round"
        android:label="Algoritmo Quiz"
        android:roundIcon="@mipmap/ic_logo_oficial_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.Cadastro"></activity>
        <activity android:name=".Activity.MainActivity" />
        <activity android:name=".Activity.Sobrevivencia" />
        <activity android:name=".Activity.Medalha"
            android:parentActivityName=".Activity.Medalha"/>
        <activity android:name=".Activity.Score"
            android:parentActivityName=".Activity.Score"/>
        <activity android:name=".Activity.Quiz" />
        <activity android:name=".Activity.Dificuldade" />
        <activity android:name=".Activity.Resultado" />
        <activity android:name=".Activity.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>